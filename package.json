{
  "name": "ember-tagify",
  "version": "0.0.12",
  "description": "An Ember addon that wraps the Tagify component.",
  "keywords": [
    "ember-addon",
    "tagify",
    "tag input"
  ],
  "repository": "https://github.com/redislabs/ember-tagify",
  "license": "MIT",
  "author": {
    "name": "Slava Vitebski",
    "email": "vitebski@gmail.com",
    "url": "https://github.com/vitebski"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "deploy": "ember build --environment production && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "@yaireo/tagify": "^3.20.0",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.1",
    "@glimmer/tracking": "^1.0.2",
    "@glimmer/component": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@ember/optional-features": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.1",
    "ember-cli": "~3.21.2",
    "ember-cli-addon-docs": "^0.10.0",
    "ember-cli-addon-docs-yuidoc": "^0.2.4",
    "ember-cli-code-coverage": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-data": "~3.21.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "v5.0.0-beta.4",
    "@ember/test-helpers": "^2.0.0",
    "qunit": "^2.11.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.2.3",
    "ember-template-lint": "^2.13.0",
    "ember-try": "^1.4.0",
    "ember-decorators": "^6.1.1",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^v3.2.1",
    "release-it": "^14.0.4",
    "release-it-lerna-changelog": "^2.4.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://redislabs.github.io/ember-tagify/#/docs/components/ember-tagify"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
